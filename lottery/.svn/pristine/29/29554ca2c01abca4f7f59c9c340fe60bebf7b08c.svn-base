                    <p id="divide">1/2</p>
                    <p id="ride">X2</p>
                    <p id="maximum">MAX</p>
                </div>
            </div>
        </section>
        <div class="login">
            <p id="loginBtn" data-state="1">登录</p>
        </div>
        <section class="record">

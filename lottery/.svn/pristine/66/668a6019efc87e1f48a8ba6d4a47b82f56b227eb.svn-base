input[type=range] {
    -webkit-appearance: none;
    width: 1.8rem;
    height: .1rem;
    background: -webkit-linear-gradient(left, #FF564F , #FF8523); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #FF564F, #FF8523); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #FF564F, #FF8523); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #FF564F , #FF8523); /* 标准的语法（必须放在最后） */
}
input[type=range]:focus {
    outline: none;
}
input[type=range]::-webkit-slider-thumb {
    -webkit-appearance: none;
}  
input[type=range]::-webkit-slider-thumb {
    -webkit-appearance: none;
    height: .22rem;
    width: .22rem;
    margin-top: -0.33rem; 
   border-radius: .02rem;
   transform: rotate(45deg);
    background: -webkit-linear-gradient(left, #FF564F , #FF8523); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #FF564F, #FF8523); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #FF564F, #FF8523); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #FF564F , #FF8523); /* 标准的语法（必须放在最后） */
}
input[type=range]::-moz-range-thumb {
    -webkit-appearance: none;
}  
input[type=range]::-moz-range-thumb {
    -webkit-appearance: none;
    height: .22rem;
    width: .22rem;
    margin-top: -0.33rem; 
   border-radius: .02rem;
   transform: rotate(45deg);
    background: -webkit-linear-gradient(left, #FF564F , #FF8523); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #FF564F, #FF8523); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #FF564F, #FF8523); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #FF564F , #FF8523); /* 标准的语法（必须放在最后） */
}
input[type=range]::-ms-track {
    border-color: transparent; /*去除原有边框*/
    color: transparent; /*去除轨道内的竖线*/
}

input[type=range]::-ms-thumb {
    height: .22rem;
    width: .22rem;
    margin-top: -0.33rem; 
   border-radius: .02rem;
   transform: rotate(45deg);
    background: -webkit-linear-gradient(left, #FF564F , #FF8523); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #FF564F, #FF8523); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #FF564F, #FF8523); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #FF564F , #FF8523); /* 标准的语法（必须放在最后） */
}

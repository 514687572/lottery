/* 内容区域 */
#rulerContainer{
    width: 1000px;
    height: auto;
    background-color: #252a48;
    box-shadow: 4px 4px 2px 1px #20223b;
    margin: auto;
    margin-top:19px;
    margin-bottom:69px; 
}
/* 标题头 */
#rulerTitle{
    width: 942px;
    height:81px;
    line-height: 81px;
    display: flex;
    justify-content: space-between;
    padding: 0 29px; 
    background-color: #282e50;
    color: white;
}
#rulerTitle>h1{
    font-size: 34px;
    font-weight: normal;
}
#rulerTitle>span{
    font-size: 16px;
}
#rulerTitle>span:hover{
    cursor: pointer;
}
/* 开奖号码详解 */
#awardNumberI{
    width:942px;
    height:344px;
    margin: auto;
    margin-top: 28px;
    border-bottom: 1px dotted #3c436e;
}
#awardNumberI>p{
    color: white;
    width: 843px;
    height: 43px;
    line-height: 43px;
    font-size: 12px;
}
#awardNumberI>p:nth-child(1)>i{
    width: 40px;
    height: 40px;
    background-color:#3b405b;
    border-radius: 40px;
    background-image: url("../images/img01.png");
    display: inline-block;
    background-size: cover;
    vertical-align: middle;
    margin-right: 9px;
}
#awardNumberI>p:nth-child(1){
    font-size: 16px;
}
#awardNumberI>p:nth-child(2){
    font-size: 14px;
    margin-left: 47px;
}
#awardNumberI>p:nth-child(3){
    margin-left: 47px;
    color: #7786d9;
}
#awardNumberI>p:nth-child(4){
    font-size: 14px;
    margin-left: 47px;
    margin-top: 30px;
}
#awardNumberI>p:nth-child(5){
    margin-left: 47px;
    color: #7786d9;
    margin-bottom: 50px;
}

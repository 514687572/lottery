package com.stip.net.entity;

import java.math.BigDecimal;

/**
 * TOP币流水
 */
public class TopFlow {
	private Long id;// 自增主键
	private String userId;// 用户名
	private String gameType;// 游戏类型
	private BigDecimal eos;// 投注EOS金额
	private String per;// 返TOP比例
	private BigDecimal top;// 返TOP金额
	private long timeSpt;// 流水产生时间

	public TopFlow() {
		super();
	}

	public TopFlow(String userId, String gameType, BigDecimal eos, String per, BigDecimal top, long timeSpt) {
		this.userId = userId;
		this.gameType = gameType;
		this.eos = eos;
		this.per = per;
		this.top = top;
		this.timeSpt = timeSpt;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getGameType() {
		return gameType;
	}

	public void setGameType(String gameType) {
		this.gameType = gameType;
	}

	public BigDecimal getEos() {
		return eos;
	}

	public void setEos(BigDecimal eos) {
		this.eos = eos;
	}

	public String getPer() {
		return per;
	}

	public void setPer(String per) {
		this.per = per;
	}

	public BigDecimal getTop() {
		return top;
	}

	public void setTop(BigDecimal top) {
		this.top = top;
	}

	public long getTimeSpt() {
		return timeSpt;
	}

	public void setTimeSpt(long timeSpt) {
		this.timeSpt = timeSpt;
	}
}
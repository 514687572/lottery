body {
  background-color: #212540;
}
#inviteContainer{
  display:none;
}
/* 内容部分 */
#inviteContent {
  width: 942px;
  height: 1388px;
  background-color: #252a48;
  margin: auto;
  margin-top: 36px;
}
#inviteContainer {
  width: 1000px;
  height: 1496px;
  background-color: #252a48;
  margin: auto;
  margin-top: 22px;
}
#inviteHead {
  width: 940px;
  padding: 0 30px;
  background-color: #282e50;
  height: 86px;
  display: flex;
  justify-content: space-between;
  color: white;
  line-height: 86px;
}
#inviteHead > p {
  font-size: 32px;
}
#inviteHead > span {
  font-size: 14px;
}
#inviteHead > span:hover{
  cursor: pointer;
}
#invitePart1 {
  width: 946px;
  height: 388px;
  border: 1px dotted #3b405b;
  margin: auto;
}
#newCome {
  width: 899px;
  height: 93px;
  border-bottom: 1px dotted #3b405b;
  margin: auto;
  color: white;
  display: flex;
}
#newCome > div:nth-child(1) {
  width: 124px;
  height: 93px;
  line-height: 93px;
}
#newCome > div:nth-child(1) > span:nth-child(1) {
  font-size: 46px;
}
#newCome > div:nth-child(2) > h3 {
  font-size: 20px;
  margin-top: 20px;
  font-weight: lighter;
}
#newCome > div:nth-child(2) > p {
  font-size: 14px;
  margin-top: 14px;
  font-weight: lighter;
  color: #4f5993;
}
#inviteFriends {
  width: 899px;
  height: 139px;
  border-bottom: 1px dotted #3b405b;
  margin: auto;
  color: white;
  display: flex;
}
#inviteFriends > div:nth-child(1) {
  width: 124px;
  height: 139px;
  line-height: 139px;
}
#inviteFriends > div:nth-child(2) {
  width: 780px;
  height: 139px;
}
#inviteFriends > div:nth-child(1) > span:nth-child(1) {
  font-size: 46px;
}
#inviteFriends > div:nth-child(2) > h3 {
  font-size: 20px;
  margin-top: 16px;
  font-weight: lighter;
}
#inviteFriends > div:nth-child(2) > p {
  font-size: 14px;
  margin-top: 8px;
  font-weight: lighter;
  color: #4f5993;
}
#getAward {
  width: 899px;
  height: 139px;
  margin: auto;
  color: white;
  display: flex;
}
#getAward > div:nth-child(1) {
  width: 124px;
  height: 139px;
  line-height: 139px;
}
#getAward > div:nth-child(1) > span:nth-child(1) {
  font-size: 46px;
}
#getAward > div:nth-child(2) > h3 {
  font-size: 20px;
  margin-top: 10px;
  font-weight: lighter;
}
#getAward > div:nth-child(2) > p {
  font-size: 14px;
  margin-top: 8px;
  font-weight: lighter;
  color: #4f5993;
}
#invitePart2 {
  width: 946px;
  height: 301px;
  border: 1px dotted #3b405b;
  margin-top: 27px;
  text-align: center;
}
#invitePart2 > h1 {
  color: white;
  font-weight: lighter;
  font-size: 22px;
  margin-top: 24px;
}
#inviteDetails {
  width: 685px;
  height: 187px;
  margin: auto;
  margin-top: 28px;
}
#icon {
  width: 423px;
  height: 44px;
  margin: auto;
  display: flex;
  justify-content: space-between;
}
#icon > i {
  width: 44px;
  height: 44px;
  border-radius: 44px;
  display: inline-block;
}
#icon > i:nth-of-type(1) {
  background:#3e425c url('../images/send.svg') no-repeat center;
  background-size: 20px 20px;
}
#icon > i:nth-of-type(2) {
  background:#3e425c url('../images/loginOut.svg') no-repeat center;
  background-size: 20px 20px;
}
#icon > i:nth-of-type(3) {
  background:#3e425c url('../images/friends.svg') no-repeat center;
  background-size: 20px 20px;
}
.line {
  width: 109px;
  height: 1px;
  border-top: 1px solid #4d5569;
  margin-top: 22px;
}
#rulerWords {
  width: 554px;
  height: 64px;
  display: flex;
  justify-content: space-between;
  margin: auto;
  margin-top: 16px;
}
#rulerWords > p {
  text-align: center;
  width: 160px;
  height: 64px;
}
#rulerWords > p > span:nth-child(1) {
  font-size: 14px;
  color: white;
}
#rulerWords > p > span:nth-of-type(2) {
  font-size: 12px;
  color: #414977;
}
#inviteLink {
  width: 453px;
  height: 29px;
  background-color: #3a3953;
  margin: auto;
  margin-top: 33px;
  padding: 6px;
  display: flex;
  justify-content: space-between;
}
#linkInput {
  width: 364px;
  height: 28px;
  padding-left: 8px;
  line-height: 28px;
  color: white;
  font-size: 12px;
  background-color: #51566d;
  border: none;
}
#copyUrl {
  width: 74px;
  height: 28px;
  text-align: center;
  line-height: 28px;
  color: white;
  font-size: 16px;
  background: linear-gradient(to right, #ff5c4a, #ff8325);
  border: none;
}
#copyUrl:hover {
  cursor: pointer;
}
#invitePart3 {
  width: 884px;
  height: 580px;
  border: 1px dotted #3b405b;
  margin-top: 27px;
  text-align: center;
  padding: 0 31px;
}
#invitePart3 > p {
  color: white;
  font-size: 24px;
  display: flex;
  justify-content: space-between;
  margin-top: 20px;
}
#myFriendsBox {
  width: 878px;
  height: 404px;
  margin: auto;
  margin-top: 20px;
}
#invitePart3 > p > span {
  font-size: 14px;
  margin-top: 12px;
}

  color: #4f5993;
}

.myFriends {
  width: 878px;
  height: 53px;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px dotted #51566d;
}
.myFriends > div:nth-child(1) {
  width: 72px;
  height: 53px;
  text-align: center;
  line-height: 53px;
  color: #4f5993;
}
.myFriends > div:nth-child(2) {
  width: 146px;
  height: 53px;
  text-align: center;
  line-height: 53px;
  color: #4f5993;
}
.myFriends > div:nth-child(3) {
  width: 50px;
  height: 53px;
  text-align: center;
  line-height: 53px;

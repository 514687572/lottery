}

#pageTurn {
  width: 100px;
  height: 30px;
  display: flex;
  justify-content: space-between;
  margin-top: 35px;
  line-height: 30px;
  float: right;
}
#prePage {
  width: 30px;
  height: 30px;
  background-color: #343c68;
  border-radius: 5px;
}
#prePage:hover{
  cursor: pointer;
}
#nextPage {
  width: 30px;
  height: 30px;
  background-color: #343c68;
  border-radius: 5px;
}
#nextPage:hover{
  cursor: pointer;
}
#curpage {
  color: white;
  font-size: 14px;
}


/* 邀请好友界面 */
#Invite{
    width: 1000px;
    margin-top: 20px;
    margin-left: auto;
    margin-right: auto;
    background-color: #252A48;
    margin-bottom: 50px;
    display: none;
}
.InviteHeader{
    color: #ffffff;
    background-color: #282E50;
    height: 80px;
    display: flex;
    justify-content: space-between;
    padding: 0 30px;
}
.InviteHeader>p:nth-of-type(1){
    font-size: 32px;
    line-height: 80px;
}
.InviteHeader>p:nth-of-type(2){
    font-size: 14px;
    line-height: 80px;
}
.briefIntroduction{
    padding: 0 30px;
    margin-top: 30px;
}
.briefIntroduction>div{
    border: 1px solid #383E58;
}
.briefIntroduction1-1{
    padding: 0 30px;
}
.briefIntroduction1-1>div{
    display: flex;
    padding: 0 28px;
}
.briefIntroduction1-1>div>div:nth-of-type(1){
    font-size: 40px;
    color: #ffffff;
    width: 150px;
}
.briefIntroduction1-1>div>div:nth-of-type(1) span{
    font-size: 20px;
}
.briefIntroduction1-1>div>div:nth-of-type(2)>p{
    font-size: 14px;
    color: #7786d9;
}
.briefIntroduction1-1>div>div:nth-of-type(2)>p:nth-of-type(1){
    font-size: 16px;
    color: #ffffff;
    margin-bottom: 14px;
}
.briefIntroduction1-1>div:nth-of-type(1){
    padding-top: 20px;
    padding-bottom: 20px;
    border-bottom: 1px solid #383E58;
}
.briefIntroduction1-1>div:nth-of-type(2){
    padding-top: 20px;
    padding-bottom: 20px;
    border-bottom: 1px solid #383E58;
}
.briefIntroduction1-1>div:nth-of-type(3){
    padding-top: 28px;
    padding-bottom: 30px;
}
.theInvitationWay{
    padding: 0 30px;
    margin-top: 30px;
}
.theInvitationWay1-1{
    border: 1px solid #383E58;
    padding-bottom: 33px;
}
.theInvitationWay1-1>div{
    width: 560px;
    margin-left: auto;
    margin-right: auto;
}
.theInvitationWay1-1>div>p:nth-of-type(1){
    text-align: center;
    color: #ffffff;
    margin-top: 30px;
    margin-bottom: 30px;
    font-size: 18px;
}
.theInvitationWay2-1{
    width: 425px;
    margin-left: auto;
    margin-right: auto;
    display: flex;
    justify-content: space-between;
}
.theInvitationWay2-1>p:nth-child(odd){
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background-color: #3A3F5B;
    position: relative;
}
.theInvitationWay2-1>p:nth-child(even){
    width: 110px;
    border-bottom: 1px solid #383E58;
    margin-bottom: 20px;
}
.theInvitationWay2-1>p:nth-of-type(1)>img{
    position: absolute;
    top: 10px;
    left: 11px;
}
.theInvitationWay2-1>p:nth-of-type(3)>img{
    position: absolute;
    top: 12px;
    left: 11px;
}
.theInvitationWay2-1>p:nth-of-type(5)>img{
    position: absolute;
    top: 12px;
    left: 11px;
}
.theInvitationWay3-1{
    display: flex;
    justify-content: space-between;
    margin-top: 15px;
    padding: 0 5px;
}
.theInvitationWay3-1>p{
    width: 165px;
    text-align: center;
    color: #ffffff;
}
.theInvitationWay4-1{
    display: flex;
    justify-content: space-between;
    margin-top: 10px;
    /* padding: 0 5px; */
}
.theInvitationWay4-1>div{
    width: 165px;
    color: #7786d9;
    font-size: 12px;
}
.theInvitationWay4-1>div>p{
    text-align: center;
}
.kyrieandrewirving{
    width: 100%;
    height: 40px;
    background-color: #303552;
    margin-top: 26px;
    display: flex;
    padding: 5px;
    box-sizing: border-box;
}
.kyrieandrewirving>input{
    width: 100%;
    background-color: #4F536C;
    border: #4F536C;
    outline:none;
}
.kyrieandrewirving>p{
    width: 75px;
    height: 30px;
    color: #ffffff;
    line-height:30px;
    text-align: center;
    margin-left: 5px;
    background: -webkit-linear-gradient(left, #FF564F , #FF8523); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #FF564F, #FF8523); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #FF564F, #FF8523); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #FF564F , #FF8523); /* 标准的语法（必须放在最后） */
}
.subordinate{
    padding: 0 30px 60px 30px;
    margin-top: 30px;
}
.subordinate>div{
    border: 1px solid #383E58;
    padding:0 28px;
}
.subordinate1-1{
    display: flex;
    justify-content: space-between;
    color: #ffffff;
    height: 30px;
    margin-top: 28px;
    margin-bottom: 18px;
}
.subordinate1-1>p{
    line-height: 30px;
}
.subordinate1-1>p:nth-of-type(1){
    font-size: 22px;
}
.subordinateTh{
    width: 100%;
    height: 50px;
    background-color: #343D68;
    color: #7786d9;
    display: flex;
}
.subordinateTh>li{
    line-height: 50px;
}
.subordinateTh>li:nth-of-type(1){
    width: 75px;
    text-align: center;
    margin-right: 36px;
}
.subordinateTh>li:nth-of-type(2){
    width: 180px;
    margin-right: 28px;
}
.subordinateTh>li:nth-of-type(3){
    width: 60px;
    text-align: center;
    margin-right: 140px;
}
.subordinateTh>li:nth-of-type(4){
    width: 90px;
    text-align: center;
    margin-right: 150px;
}
.subordinateTh>li:nth-of-type(5){
    width: 100px;
    text-align: center;
}
.subuserForm{
    height: 400px;
}
.subuserForm>ul:nth-child(even){
    background-color: #272D4D;
}
.subordinateTable{
    width: 100%;
    height: 48px;
    display: flex;
    border-bottom: 1px solid #383E58;
}
.subordinateTable>li{
    line-height: 48px;
}
.subordinateTable>li:nth-of-type(1){
    width: 75px;
    text-align: center;
    margin-right: 36px;
    color: #7786d9;
}
.subordinateTable>li:nth-of-type(2){
    width: 180px;
    margin-right: 28px;
    color: #ffffff;
}
.subordinateTable>li:nth-of-type(3){
    width: 60px;
    text-align: center;
    margin-right: 80px;
    color: #ffffff;
}
.subordinateTable>li:nth-of-type(4){
    width: 150px;
    text-align: right;
    margin-right: 90px;
    color: #D54D4D;
}
.subordinateTable>li:nth-of-type(5){
    width: 148px;
    text-align: right;
    color: #D54D4D;
}
#inviteInput{
    padding-left: 15px;
    color: #ffffff;
}
.subuserForm{
    overflow: auto;
}
/* 分页 */
.page{
    display: flex;
    margin-top: 30px;
    margin-bottom: 40px;
    justify-content: flex-end;
    color: #7786d9;
}
#upPage{
    width: 30px;
    height: 30px;
    color: #ffffff;
    text-align: center;
    line-height: 30px;
    background-color: #343D68;
    margin-right: 10px;
}
#nextPage{
    width: 30px;
    height: 30px;
    color: #ffffff;
    text-align: center;
    line-height: 30px;
    background-color: #343D68;
    margin-left: 10px;
}
.page>p:nth-of-type(2),p:nth-of-type(3),p:nth-of-type(4){
    margin-top: 5px;
}
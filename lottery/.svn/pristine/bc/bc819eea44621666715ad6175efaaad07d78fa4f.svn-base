    function commissions() {//获取投注金额，显示预计奖金
        let inputBoxValue = $("#inputBox").val() - 0;
        if (inputBoxValue >= 100) {
            $("#inputBox").val(100);
            inputBoxValue = 100;
        } else if (inputBoxValue < 0) {
            $("#inputBox").val(0.1);
            inputBoxValue = 0.1;
        } else if (inputBoxValue == 0) {
            $("#inputBox").val(0.1);
            inputBoxValue = 0.1;
        }

        let odds = $("#odds").text() - 0;
        let bonus = (inputBoxValue * odds).toFixed(3) - 0;
        $("#bonus").text(bonus);
    };



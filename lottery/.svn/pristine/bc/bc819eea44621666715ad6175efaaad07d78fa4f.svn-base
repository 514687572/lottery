        mag = evt;
        // console.log("mag",evt)
        received_msg = evt.data;
        received_msg = JSON.parse(received_msg);
        // console.log("from", received_msg);
        if (received_msg.from == "diceBlock") {//区块消息
            theLotteryResults();
            theLotteryList();

        }
        else if (received_msg.from == "diceUserPrize") {//发送给某个用户

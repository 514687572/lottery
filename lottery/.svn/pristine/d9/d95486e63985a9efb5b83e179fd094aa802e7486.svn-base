package com.stip.net.entity.tiger;

public class BlockInfo {
	private long block_number;// 区块号
	private String hash;// 区块哈希
	private long block_time;// 区块时间

	public BlockInfo(long block_number, String hash, long block_time) {
		this.block_number = block_number;
		this.hash = hash;
		this.block_time = block_time;
	}

	public long getBlock_number() {
		return block_number;
	}

	public void setBlock_number(long block_number) {
		this.block_number = block_number;
	}

	public String getHash() {
		return hash;
	}

	public void setHash(String hash) {
		this.hash = hash;
	}

	public long getBlock_time() {
		return block_time;
	}

	public void setBlock_time(long block_time) {
		this.block_time = block_time;
	}
}
<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@taglib prefix="ctx" uri="http://www.springframework.org/tags" %>
<%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="../css/reset.css" />
    <title>手机注册页面</title>
    <style>
    *{
        padding:0;
        margin:0;
    }
    html{
        font-size:0.16rem;
    }
    </style>
</head>
<body style="background:#12141e;font-family:Arial, Helvetica, sans-serif;">
    <div style="background-image:url('../images/bg.png');background-repeat:no-repeat;background-size:100% 100%;height:2.5rem;padding-top:0.2rem;text-align: center;">
        <p style="font-size:0.16rem;color:white;text-align: center;">欢迎来到EOS Bank</p>
        <img src="../images/circle.png" alt="" style="width:0.86rem;height:0.86rem;">
        <div style="width:2.46rem;height:0.48rem;line-height:0.48rem;text-align:center;color:white;font-weight:bloder;font-size:0.26rem;margin:auto;border:0.01rem solid #363840;">
            <h1>EOS Bank</h1>
        </div>
    </div>
    <div style="width:2.11rem;height:1.11rem;margin:auto;position:relative;margin-top:0.74rem;">
        <button  id="register" style="position:absolute;top:0;width:2.11rem;height:0.41rem;line-height:0.41rem;text-align:center;color:white;font-size:0.14rem;border-radius:0.05rem;background: linear-gradient(to right, #ff564f , #ff8523);border:none;outline:none;">注册</button>
        <button  id="payfor" style="position:absolute;bottom:0;width:2.11rem;height:0.41rem;line-height:0.41rem;text-align:center;color:white;font-size:0.14rem;border-radius:0.05rem;background: #343c68;border:none;outline:none;">充值</button>
    </div>

    <script src="../js/jquery.js"></script>
    <script src="../js/wapAutoSize.js"></script> 
    <script type="text/javascript">
        window.onload=function(){
            $("#register").click(function(){
                window.location.href="../html/wapRegisterOneStep.html"
            });
            $("#payfor").click(function(){
                window.location.href="../html/wapPayfor.html"
            });
        }
    </script>
</body>
</html*>